<template name="fallsLayout">
    <view class="fallsLayout">
        <view class="img_item_left">
            <block wx:for="{{colLeft}}" wx:key="index">
                <image src="{{item.url}}" data-imageid = "{{item.id}}" style="width:100%;height:{{item.height}}px"></image>
            </block>
        </view>
        <view class="img_item_right">
            <block wx:for="{{colRight}}" wx:key="id">
                <image src="{{item.url}}" data-imageid = "{{item.id}}" style="width:100%;height:{{item.height}}px"></image>
            </block>
        </view>
    </view>
</template>